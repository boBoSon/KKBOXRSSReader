# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'KKBOXRSSReader' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for KKBOXRSSReader
  # Parse RSS
  pod 'FeedKit'
  # Downloading and caching images from the web
  pod 'Kingfisher'
  # Layout programmatically
  pod 'SnapKit'
  # Reactive programming
  pod 'RxSwift'
  pod 'RxCocoa'
  # pod 'RxDataSources'
  # HTTP networking
  pod 'Alamofire'
  # Local database (not used)
  # pod 'RealmSwift'

  target 'KKBOXRSSReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'KKBOXRSSReaderUITests' do
    # Pods for testing
  end

end
